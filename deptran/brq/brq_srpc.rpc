
namespace rococo;

abstract service Brq {

  defer Dispatch(SimpleCommand cmd |
                 i32 res,
                 map<i32, Value> output,
                 BrqGraph ret_graph);

  defer Finish(cmdid_t id,
               BrqGraph |
               map<uint32_t, map<i32, Value>> output);

  defer Inquire(cmdid_t txn_id |
                BrqGraph ret_graph);
}